{
    "version": 2,
    "builds": [
      {
        "src": "api/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/app.js" },
      { "src": "/(.*)", "dest": "/client/dist/index.html" }
    ]
  }
  